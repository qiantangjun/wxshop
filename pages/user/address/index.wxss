/* pages/user/address/index.wxss */

.emptynone {
  font-size: 32rpx;
  text-align: center;
  color: #999;
  margin: 80rpx 100rpx 0;
  padding-top: 250rpx;
  background: url(data:image/png;base64,) no-repeat 50% 0;
  background-size: 200rpx 200rpx;
}

.orderaddress {
  border-bottom: 5px solid #f5f5f5;
  position: relative;
}

.orderaddress .tetx {
  line-height: 70rpx;
  font-size: 32rpx;
  border-bottom: 1px solid #f5f5f5;
  padding-left: 15rpx;
  display: block;
}

.addressinfo {
  width: 720rpx;
  padding: 15rpx;
}

.addressinfo text {
  display: block;
  font-size: 30rpx;
  line-height: 1.5;
}

.addressinfo .addrs {
  padding: 10rpx 0;
}

.orderaddress .icon {
  content: "";
  position: absolute;
  right: 12rpx;
  top: 50%;
  width: 60rpx;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}

.ord {
  padding-top: 10rpx;
}

.wrap {
  overflow: hidden;
  border-top: 1px solid #f5f5f5;
}

.checkbox {
  font-size: 30rpx;
  float: left;
  height: 70rpx;
}

.checkbox .ick {
  width: 40rpx;
  height: 40rpx;
  float: left;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  margin: 15rpx 15rpx 0 15rpx;
}

.checkbox .ick2 {
  width: 40rpx;
  height: 40rpx;
  float: left;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  margin: 15rpx 15rpx 0 15rpx;
}

.checkbox  text {
  float: left;
  line-height: 70rpx;
}

.chose-add {
  float: right;
  width: 220rpx;
}

.chose-add navigator {
  width: 40%;
  float: right;
  font-size: 30rpx;
  line-height: 70rpx;
  margin-left: 5rpx;
}

.addnewaddress {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #ff6700;
}
