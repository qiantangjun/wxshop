<!--pages/user/address/index.wxml-->
<view class='container'>
  <view class='orderaddress' wx:for="{{address}}" wx:for-item="item" wx:key="index" id="{{item.id}}" >
    <view class='addressinfo' bindtap='gopay' id='{{item.id}}' data-index="{{index}}">
      <text class='name'>{{item.linkMan}}  {{item.mobile}}</text>
      <text class='addrs'>{{item.provinceStr}}{{item.cityStr}}{{item.areaStr}}{{item.address}}</text>
      <view class='ord' wx:if="{{item.isDefault==true}}">
        <image src='data:image/jpeg;base64,'
          mode='widthFix'></image>
      </view>
    </view>
     <navigator class='icon' url='choseaddress/chose?id={{item.id}}'>
        <image src='../../../images/address.png' mode='widthFix'></image>
      </navigator>
  </view>
  <view class='addnewaddress'>
    <navigator url='choseaddress/chose'>新增地址</navigator>
  </view>
  <view class='emptynone' wx:if="{{!address}}">
    <text class='text'>暂无可用的地址,请先添加地址</text>
  </view>
</view>