<!--pages/car/index.wxml-->
<view class='container'>
  <navigator class='orderaddress' wx:for="{{address}}" wx:for-item="item" wx:key="index" wx:if="{{item.isDefault==true&&address.length>0&&car.length>0}}" url="../user/address/index" data-index="{{index}}">
    <text class='tetx'>运送至:</text>
    <view class='addressinfo'>
      <text class='name'>{{item.linkMan}} {{item.mobile}}收</text>
      <text class='addrs'>{{item.provinceStr}}{{item.areaStr}}{{item.address}}</text>
      <view class='icon'>
      </view>
      <view class='ord' wx:if="{{item.isDefault==true}}">
        <image src='data:image/jpeg;base64,'
          mode='widthFix'></image>
      </view>
    </view>
  </navigator>
  <navigator class='orderaddress' wx:for="{{address2}}" wx:for-item="item" wx:key="index" wx:if="{{address2.length>0&&car.length>0}}" url="../user/address/index" data-index="{{index}}">
    <text class='tetx'>运送至:</text>
    <view class='addressinfo'>
      <text class='name'>{{item.linkMan}} {{item.mobile}}收</text>
      <text class='addrs'>{{item.provinceStr}}{{item.areaStr}}{{item.address}}</text>
      <view class='icon'>
      </view>
      <view class='ord' wx:if="{{item.isDefault==true}}">
        <image src='data:image/jpeg;base64,'
          mode='widthFix'></image>
      </view>
    </view>
  </navigator>
  <navigator class='addlist' wx:if="{{address.length<1&&address2.length<1}}" url="../user/address/choseaddress/chose">添加默认收货地址</navigator>
  <view class='car-item-box' wx:if="{{car.length>0}}">
    <view class='product-item' wx:for="{{car}}" wx:for-item="item" wx:key="index" data-index="{{index}}">
      <view class='item-box'>
        <view class='item-check {{item.selected?"check":""}}' data-index="{{index}}" bindtap='selectedlist'></view>
        <view class='list-img'>
          <image src='{{item.pic}}' mode='widthFix'></image>
        </view>
        <view class='info-tit'>
          <text class='name'>{{item.title}}</text>
          <text class='price-without'>售价:<text>{{item.price}}</text>
          <text>数量:</text>{{item.number}}
         </text>
          <view class='xm-input-number'>
            <view class='input-sub' bindtap='subnum' data-index="{{index}}">-</view>
            <input class='inout-num' value='{{item.number}}' disabled='true' type='number'></input>
            <view class='input-add' bindtap='addnum' data-index="{{index}}">+</view>
          </view>
          <view class='delete' bindtap='delecart'>
            <image src='data:image/png;base64,'
              mode='widthFix'></image>
          </view>
        </view>
      </view>
    </view>
  </view>
      <view class='footer-pay' wx:if="{{car.length>0}}">
      <view class='item2' url='../index/index'>
        <view class='item-check {{selectedAll?"check":""}}' bindtap='selectedAll'></view>全选</view>
      <view class='item'>
        <text class='item-pri'>共{{totalnum}}件 金额:</text>
        <text class='all'>{{totalPrice}}元</text>
      </view>
      <view class='item3' wx:if="{{address.length>0||address2.length>0}}"><view bindtap='gopay'>去结算</view></view>
      <view class='item3 item4' wx:else>先选择地址</view>
    </view>
  <view class='empty-box' wx:if="{{car.length<1}}">
    <image src='data:image/png;base64,'
      mode='widthFix'></image>
    <text class='span'>购物车空空如也</text>
    <view class='goindex' bindtap='gotoindex'>去商城逛逛吧</view>
  </view>
</view>