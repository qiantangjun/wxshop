/* pages/prolist/index.wxss */

.container {
  margin: 0;
  padding: 0;
}
.emptynone {
  font-size: 32rpx;
  text-align: center;
  color: #999;
  width:300rpx;
  padding-top:370rpx;
  background: url(data:image/png;base64,) no-repeat  center center;
  background-size:100% auto;
  display:block
}
.emptynone .text{padding-top:20rpx}
.header {
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: #f5f5f5;
}

.index-content {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width:100%;
}

.index-content .item {
  width: 49.5%;
  margin-bottom: 10rpx;
}

.index-content .title {
  font-size: 32rpx;
  line-height: 1.5;
  padding-left: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.index-content .smalltitle {
  font-size: 28rpx;
  line-height: 1.5;
  color: #999;
  padding-left: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.index-content .price {
  font-size: 30rpx;
  color: #ff6700;
}

.index-content .numpi {
  font-size: 28rpx;
  color: #999;
  text-decoration: line-through;
  padding-left: 20rpx;
}

.weui-loading {
  margin: 0 auto;
  width: 20px;
  height: 20px;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.more {
  font-size: 28rpx;
  text-align: center;
}
