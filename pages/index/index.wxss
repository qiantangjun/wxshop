.container {
  margin: 0;
  padding: 0;
}

.header {
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: #fff;
  position: relative;
}

.banner {
  width: 100%;
}

.header image {
  width: 80rpx;
  margin: 20rpx auto 0 auto;
}

.header-search {
  width: 90rpx;
  height: 90rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 70rpx 70rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.header-mess {
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  left: 0;
  top: 0;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: 50rpx 50rpx;
}

.header-mess .dot {
  width: 20rpx;
  height: 20rpx;
  background: #18c7da;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  position: absolute;
  left: 55rpx;
  top: 20rpx;
}

.swiper {
  width: 750rpx;
  height: 396rpx;
}

.index-icon {
  width: 100%;
  display: -webkit-flex;
  display: flex;
  padding-bottom: 5rpx;
  height: 139rpx;
}

.index-icon image {
  width: 60%;
  margin: 0 auto;
}

.index-icon .item {
  -webkit-flex: 1;
  flex: 1;
}

.index-icon .text {
  text-align: center;
  display: block;
  font-size: 32rpx;
  color: #666;
}

.index-cell {
  margin: 6rpx auto;
  width: 750rpx;
  height: 540rpx;
  position: relative;
}

.cell-left {
  width: 372rpx;
  height: 540rpx;
  float: left;
}

.cell-top {
  width: 364rpx;
  height: 267rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.cell-bottom {
  width: 364rpx;
  height: 270rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.index-section {
  width: 100%;
  margin-bottom: 6rpx;
}

.index-title, .index-title2, .index-title3, .index-title4 {
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 25rpx;
  border-left: 6rpx solid #ffac13;
}

.index-content {
  padding-top: 12rpx;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.index-content .item {
  width: 49.5%;
  margin-bottom: 10rpx;
}

.index-content .title {
  font-size: 32rpx;
  line-height: 1.5;
  padding-left: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.index-content .smalltitle {
  font-size: 28rpx;
  line-height: 1.5;
  color: #999;
  padding-left: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.index-content .price {
  font-size: 30rpx;
  color: #ff6700;
}

.index-content .numpi {
  font-size: 28rpx;
  color: #999;
  text-decoration: line-through;
  padding-left: 20rpx;
}

.index-title2 {
  border-left: 6rpx solid #83c44e;
}

.index-title3 {
  border-left: 6rpx solid #2196f3;
}

.index-title4 {
  border-left: 6rpx solid #e53935;
}

.weui-loading {
  margin: 0 auto;
  width: 20px;
  height: 20px;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.more {
  font-size: 28rpx;
}

.icon2 image {
  width: 100%;
}
